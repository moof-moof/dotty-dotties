<?xml version="1.0" encoding="utf-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info"> 
	<mime-type type="text/x-readme">
  		<comment>LÄSMEJ-dokument</comment>
  		<sub-class-of type="text/plain"/>
  		<generic-icon name="text-x-readme"/>
  		<glob weight="100" pattern="README*"/>
  		<glob weight="100" pattern="README.md"/>
  		<glob weight="100" pattern="README.txt"/>
  		<glob weight="100" pattern="LÄSMEJ*"/>
  		<glob weight="100" pattern="LÄSMEJ.md"/>
  		<glob weight="100" pattern="LÄSMEJ.txt"/>
  		<alias type="text/readme"/>
	</mime-type>
</mime-info>