<?xml version="1.0" encoding="utf-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/vnd.google-earth.kml+xml">
      <comment>KML geographic data</comment>
      <comment xml:lang="sv">KML geografisk data</comment>
      <acronym>KML</acronym>
      <expanded-acronym>Keyhole Markup Language</expanded-acronym>
      <sub-class-of type="application/xml"/>
      <!-- <generic-icon name="application-kml"/> -->
      <generic-icon name="text-xml"/>
      <alias type="application/kml"/>
      <alias type="application/x-kml+xml"/>
      <alias type="application/x-kml"/>
      <glob pattern="*.kml"/>
  </mime-type>
</mime-info>

